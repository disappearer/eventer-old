{
  "globals": {
    "EventRepository": true,
    "UserRepository": true,
    "authServiceSuccess": true,
    "testDbData": true,
    "RepositoryError": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "env": {
    "es6": true,
    "node": true,
    "jasmine": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "windows"],
    "semi": ["error", "always"],
    "no-console": 1
  }
}
